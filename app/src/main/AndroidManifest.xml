<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zybooks.finalproject">

    <application android:name=".PHINN"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FinalProject">
        <activity android:name=".QuestionnaireActivity" />
        <activity android:name=".GoalsActivity" />
        <activity android:name=".ResourcesActivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".MoodTrackerActivity" />
        <activity android:name=".NotificationSettingsActivity" />
        <activity android:name=".HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>